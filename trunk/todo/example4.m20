; Пример обращения к стандартной подпрограмме.
; Вычисляем sin(), обращаясь к СП 0005.

:0020			; Данные
=.5			; x
=0			; sin x

:0100			; Команды

0 50 0010 7500 7767	; Загружаем ИС-2
0 70 7500 0000 0000

0 16 0103 7501 7610	; Вызов y := sin(x)
0 75 0020 0005 0021	; адрес x, номер СП, адрес y

0 50 2100 0000 0021	; Десятичная печать двух чисел
0 70 0020 0000 0000

0 77 0000 0000 0000 	; Остановка

@0100			; Старт
