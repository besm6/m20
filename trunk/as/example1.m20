; Пример 1 из http://vak.ru/doku.php/proj/besm/m20
; Решение уравнения 1/32 x^5 - 3/4 x^2 - x + 4.005 = 0

:0020			; Данные
=2			; 0020 - x0
=4.005			; 0021
=0.03125		; 0022 - 1/32
=0.0001			; 0023 - ε
=0.75			; 0024 - 3/4

:0100			; Команды. Рабочие ячейки начинаются с адреса 4000.
0 00 0020 0000 4000 	; Засылка x[0]
0 05 4000 4000 4001 	; x[i]^2
0 05 4001 4000 4002 	; x[i]^3
0 05 4002 4001 4002 	; x[i]^5
0 05 0022 4002 4002 	; 1/32 x[i]^5
0 05 4001 0024 4001 	; 3/4 x[i]^2
0 02 4002 4001 4001 	; 1/32 x[i]^5 - 3/4 x[i]^2
0 01 0021 4001 4001 	; x[i+1] = 1/32 x[i]^5 - 3/4 x[i]^2 + 4.005
0 02 4001 4000 4002 	; x[i+1] - x[i]
0 03 4002 0023 0000 	; | x[i+1] - x[i] | - | ε |
0 76 4001 0101 4000 	; Пересылка x[i+1] в 4000
0 77 0000 0000 0000 	; Остановка

@0100			; Старт
